---
import Post from "./Post.astro"
const { posts } = Astro.props
---

<ul>
    {posts.map((post) => (
        <Post url={post.url} title={post.frontmatter.title} pubDate={post.frontmatter.pubDate} />)
    )}
</ul>

<style>
    ul {
        list-style-type: none;
        padding-left: 0;
    }
</style>
